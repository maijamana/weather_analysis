   "outputs": [],
   "source": [
    "df = Research.read_information('weatherAUS.csv')\n",
    "df['Date'] = pd.to_datetime(df['Date'])\n",
    "df = Research.pd_filter_year(df, 'Date', 2010, 'Date', 2017)"
   ]
